{
  "name": "generate-project",
  "description": "Scaffold out complete code projects from the command line, or use this generator as a plugin in other generators to provide baseline functionality.",
  "version": "0.3.0",
  "homepage": "https://github.com/generate/generate-project",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-project",
  "bugs": {
    "url": "https://github.com/generate/generate-project/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "LICENSE",
    "README.md",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "generate-contributing": "^0.1.3",
    "generate-defaults": "^0.5.0",
    "generate-editorconfig": "^0.1.9",
    "generate-eslint": "^0.2.1",
    "generate-gitattributes": "^0.1.1",
    "generate-gitignore": "^0.1.2",
    "generate-install": "^0.1.8",
    "generate-license": "^0.3.5",
    "generate-package": "^0.4.4",
    "generate-readme": "^0.2.1",
    "generate-travis": "^0.2.0",
    "is-valid-app": "^0.2.0"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "generate": "^0.9.8",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3",
    "npm-install-global": "^0.1.2",
    "verb-generate-readme": "^0.1.28",
    "verb-trees": "^0.1.0"
  },
  "keywords": [
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "command-line",
    "create",
    "dev",
    "development",
    "framework",
    "front",
    "frontend",
    "generate",
    "generate-generator",
    "generate-plugin",
    "generategenerator",
    "generateplugin",
    "generator",
    "init",
    "initialize",
    "new",
    "plugin",
    "project",
    "projects",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "template",
    "templates",
    "webapp",
    "yeoman",
    "yo"
  ],
  "lintDeps": {
    "ignore": [
      "scaffolds",
      "templates"
    ]
  },
  "verb": {
    "toc": {
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "generator",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate-dest",
        "generate-install",
        "generate-package"
      ]
    },
    "reflinks": [
      "assemble",
      "base",
      "generate",
      "generate-dest",
      "generate-editorconfig",
      "generate-eslint",
      "generate-git",
      "generate-install",
      "generate-license",
      "generate-package",
      "generate-travis",
      "gulp",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
