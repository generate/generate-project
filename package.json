{
  "name": "generate-project",
  "description": "Scaffold out complete code projects from the command line, or use this generator as a plugin in other generators to provide baseline functionality.",
  "version": "0.1.2",
  "homepage": "https://github.com/generate/generate-project",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-project",
  "bugs": {
    "url": "https://github.com/generate/generate-project/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "README.md",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "generate-collections": "^0.3.5",
    "generate-defaults": "^0.3.1",
    "generate-editorconfig": "^0.1.3",
    "generate-eslint": "^0.1.2",
    "generate-git": "^0.1.9",
    "generate-install": "^0.1.4",
    "generate-license": "^0.2.0",
    "generate-package": "^0.3.1",
    "generate-travis": "^0.1.2",
    "is-valid-app": "^0.2.0"
  },
  "devDependencies": {
    "archy": "^1.0.0",
    "delete": "^0.3.2",
    "extend-shallow": "^2.0.1",
    "generate": "^0.8.0",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3",
    "npm-install-global": "^0.1.2",
    "verb-readme-generator": "^0.1.18"
  },
  "keywords": [
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "command-line",
    "create",
    "dev",
    "development",
    "framework",
    "front",
    "frontend",
    "generate",
    "generategenerator",
    "generateplugin",
    "generator",
    "init",
    "initialize",
    "new",
    "plugin",
    "project",
    "projects",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "template",
    "templates",
    "webapp",
    "yeoman",
    "yo"
  ],
  "lintDeps": {
    "ignore": [
      "scaffolds",
      "templates"
    ]
  },
  "verb": {
    "toc": true,
    "layout": "common-minimal",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate-package",
        "generate-install",
        "generate-dest"
      ]
    },
    "reflinks": [
      "assemble",
      "base",
      "generate",
      "generate-dest",
      "generate-editorconfig",
      "generate-eslint",
      "generate-git",
      "generate-license",
      "generate-package",
      "generate-travis",
      "gulp",
      "verb",
      "verb-readme-generator",
      "generate-install"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
